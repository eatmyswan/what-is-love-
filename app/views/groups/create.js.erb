<% if @group.personal == true %>
	$('#personal .new_category').after('<%= escape_javascript render(:partial => "category", :locals => { :category => @group, :active => false }) %>');
<% elsif @group.professional == true %>
	$('#professional .new_category').after('<%= escape_javascript render(:partial => "category", :locals => { :category => @group, :active => false }) %>');
<% end %>
