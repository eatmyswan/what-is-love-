#nav {
	position: absolute;
	top: 20px;
	left: 20px;
	right: 20px;
	display: none;
}
#nav a.well {
	display: inline-block;
	background: url(<%= asset_path 'nav_well_bg.jpg' %>) repeat-x top center transparent;
	border-radius: 5px;
	box-shadow: 0 1px 1px #5e5955;
	border: 1px solid #1d1b1a;
	color: #FFF;
	font-size: 12px;
	padding: 7px 8px 0;
	height: 18px;
}
#back_to_nav {
	position: absolute;
	top: 9px;
	left: 0;
	width: 20px;
	height: 20px;
	background: url(<%= asset_path 'back_btn.png' %>) no-repeat 0 0 transparent;
	margin: 0 0 0 10px;
	display: none;
	z-index: 9;
}