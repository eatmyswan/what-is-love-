/* Slider
----------------------------------*/
.jui-2 .ui-slider { position: relative; text-align: left; }
.jui-2 .ui-slider .ui-slider-handle { position: absolute; z-index: 2; width: 1.2em; height: 1.2em; cursor: default; }
.jui-2 .ui-slider .ui-slider-range { position: absolute; z-index: 1; font-size: .7em; display: block; border: 0; }

.jui-2 .ui-slider-horizontal { height: .8em; }
.jui-2 .ui-slider-horizontal .ui-slider-handle { top: -.3em; margin-left: -.6em; }
.jui-2 .ui-slider-horizontal .ui-slider-range { top: 0; height: 100%; }
.jui-2 .ui-slider-horizontal .ui-slider-range-min { left: 0; }
.jui-2 .ui-slider-horizontal .ui-slider-range-max { right: 0; }

.jui-2 .ui-slider-vertical { width: .8em; height: 100px; }
.jui-2 .ui-slider-vertical .ui-slider-handle { left: -.3em; margin-left: 0; margin-bottom: -.6em; }
.jui-2 .ui-slider-vertical .ui-slider-range { left: 0; width: 100%; }
.jui-2 .ui-slider-vertical .ui-slider-range-min { bottom: 0; }
.jui-2 .ui-slider-vertical .ui-slider-range-max { top: 0; }
