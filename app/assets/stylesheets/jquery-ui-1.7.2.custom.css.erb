/* Component containers
----------------------------------*/
.jui-2 .ui-widget { font-family: Trebuchet MS, Tahoma, Verdana, Arial, sans-serif; font-size: 1.1em; }
.jui-2 .ui-widget input, .jui-2 .ui-widget select, .jui-2 .ui-widget textarea, .jui-2 .ui-widget button { font-family: Trebuchet MS, Tahoma, Verdana, Arial, sans-serif; font-size: 1em; }
.jui-2 .ui-widget-content { border: 1px solid #dddddd; background: #eeeeee url(<%= asset_path "jui-theme/ui-bg_highlight-soft_100_eeeeee_1x100.png" %>) 50% top repeat-x; color: #333333; }
.jui-2 .ui-widget-content a { color: #333333; }
