/* Layout helpers
----------------------------------*/
.jui-2 .ui-helper-hidden { display: none; }
.jui-2 .ui-helper-hidden-accessible { position: absolute; left: -99999999px; }
.jui-2 .ui-helper-reset { margin: 0; padding: 0; border: 0; outline: 0; line-height: 1.3; text-decoration: none; font-size: 100%; list-style: none; }
.jui-2 .ui-helper-clearfix:after { content: "."; display: block; height: 0; clear: both; visibility: hidden; }
.jui-2 .ui-helper-clearfix { display: inline-block; }
/* required comment for clearfix to work in Opera \*/
* html .ui-helper-clearfix { height:1%; }
.jui-2 .ui-helper-clearfix { display:block; }
/* end clearfix */
.jui-2 .ui-helper-zfix { width: 100%; height: 100%; top: 0; left: 0; position: absolute; opacity: 0; filter:Alpha(Opacity=0); }


/* Interaction Cues
----------------------------------*/
.jui-2 .ui-state-disabled { cursor: default !important; }


/* Icons
----------------------------------*/

/* states and images */
.jui-2 .ui-icon { display: block; text-indent: -99999px; overflow: hidden; background-repeat: no-repeat; }


/* Misc visuals
----------------------------------*/

/* Overlays */
.jui-2 .ui-widget-overlay { position: absolute; top: 0; left: 0; width: 100%; height: 100%; }



