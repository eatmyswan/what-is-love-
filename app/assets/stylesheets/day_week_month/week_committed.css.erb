#week_committed #week_plan_side {
	border-bottom: 1px solid #1F1D1C;
	margin: 0 10px;
}

#week_committed #week_plan_side .date {
	margin: 30px 0 5px 0;
	height: 100%;
	position: relative;
	display: block;
	font-size: 20px;
	color: #FFF;
	vertical-align: bottom;
	white-space: nowrap;
}
#week_committed .result_count_wrap {
	margin: 0 0 20px 0;
	font-size: 11px;
}
#week_committed .result_count_wrap .title {
	font-weight: bold;
	margin: 0 7px 0 0;
	display: inline-block;
	color: #afadac;
}
#week_committed .result_count_wrap .count {
	display: inline-block;
	color: #FFF;
}

#week_committed #day_drop_wrap {
	border-top: 1px solid #3D3A39;
	padding: 20px 0 20px;
	margin: 0 10px;
}
#week_committed .day_wrap {
	margin: 0 0 15px 0;
}
#week_committed .day_wrap .day_title_wrap {
	color: #FFF;
	margin: 0 0 7px;
	overflow: hidden;
}
#week_committed .day_wrap .day_title_wrap .day_title {
	float: left;
	margin: 0 5px 0 0;
}
#week_committed .day_wrap .day_title_wrap .must_wrap {
	float: right;
	margin: 0 10px 0 0;
	font-size: 11px;
}
#week_committed .day_wrap .day_title_wrap .total_wrap {
	float: right;
	font-size: 11px;
	margin: 0 4px 0 0;
}
#week_committed .day_wrap .day_title_wrap .must_wrap .title,
#week_committed .day_wrap .day_title_wrap .total_wrap .title {
	font-weight: bold;
	margin: 0 7px 0 0;
	display: inline-block;
	color: #AFADAC;
}
#week_committed .day_wrap .day_title_wrap .must_wrap .time,
#week_committed .day_wrap .day_title_wrap .total_wrap .time {
	font-weight: bold;
	display: inline-block;
	color: #afadac;
}
#week_committed .day_wrap .committed_wrap {
	min-height: 26px;
	background: rgba(0, 0, 0, 0.2);
	border-radius: 5px;
	box-shadow: inset 0 2px 4px rgba(0, 0, 0, 0.3);
	border-bottom: 1px solid rgba(255, 255, 255, 0.1);
}
#week_committed .day_drop_over {
	background: #0060b9 !important;
	background: -webkit-linear-gradient(top, #2a90ee 0%, #0060b9 100%) !important;
	box-shadow: inset 0 1px 0 #6fb7f9 !important;
	color: #FFF !important;
	opacity: 1.0 !important;
}
#week_committed .day_drop_over .task_wrap {
	opacity: 0 !important;
}

#week_committed .committed_wrap .task_wrap .date,
#week_committed .committed_wrap .task_wrap .duration,
#week_committed .committed_wrap .task_wrap .edit,
#week_committed .committed_wrap .task_wrap .checkbox {
	display: none !important;
}

