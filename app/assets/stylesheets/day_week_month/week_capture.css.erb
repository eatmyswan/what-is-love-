#week_capture_side {
	padding: 0 0 30px 0;
	border-bottom: 1px solid #1F1D1C;
	margin: 0 10px;
}

#week_capture_side .date_title {
	margin: 30px 0 5px 0;
	height: 100%;
	position: relative;
	display: block;
	font-size: 20px;
	color: #FFF;
	vertical-align: bottom;
	white-space: nowrap;
}
#week_capture_side .result_count_wrap {
	margin: 0 0 20px 0;
	font-size: 11px;
}
#week_capture_side .result_count_wrap .title {
	font-weight: bold;
	margin: 0 7px 0 0;
	display: inline-block;
	color: #afadac;
}
#week_capture_side .result_count_wrap .count {
	display: inline-block;
	color: #FFF;
}