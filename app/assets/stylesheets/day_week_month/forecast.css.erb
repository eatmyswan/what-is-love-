#event_edit_container {
	display: none;
	background: #FFF;
	border: 4px solid #222;
}

#wc-title {
	display: inline-block;
	max-width: 400px;
	margin: 20px 0 0 20px;
	color: white;
	font-size: 20px;
	font-weight: bold;
	text-shadow: 0 1px 2px black;
}

 .wc-container {
	font-size: 14px;	
	font-family: arial, helvetica;
}
 .wc-toolbar {
	padding: 1em;
  font-size:0.8em;
}
 .wc-toolbar .wc-nav {
  float:left;
}
 .wc-toolbar .wc-display {
  float: right;
}
 .wc-toolbar button {
	margin-top: 0;
	margin-bottom: 0;
}
 .wc-toolbar .wc-title {
  text-align:center;
  padding:0;
  margin:0;
}
 .wc-container table {
	border-collapse: collapse;
    border-spacing: 0;
}
 .wc-container table td {
	margin: 0;
	padding: 0;
}
 .wc-header{
	height: 49px;
	margin: 0 16px 0 15px;
}
 .wc-header .wc-day-column-header {
	text-align: center;
	font-size: 12px;
	padding: 30px 0 7px 0;
	border-bottom: 1px solid #CCC;
	min-height: 12px;
}
 .wc-header table{
	width: 100%;
  table-layout:fixed;
}
 .wc-grid-timeslot-header,  .wc-header .wc-time-column-header {
	width: 22px;	
	border-bottom: 1px solid transparent;
}
 .wc-header .wc-scrollbar-shim {
	width: 16px;	
	display: none; 
}

 .wc-header .wc-user-header{
	text-align: center;
	padding: 0.4em 0;
  overflow:hidden;
}
 .wc-grid-timeslot-header {
}


 .wc-scrollable-grid {
	overflow: auto;
	overflow-x: hidden !important;
	overflow-y: auto !important; 
	position: absolute;
	top: 50px;
	left: 15px;
	right: 15px;
	bottom: 0;
}
#schedule .wc-scrollable-grid {
	top: 38px;
}

 table.wc-time-slots {
	width: 100%;
	table-layout: fixed;
	cursor: default;
	border-right: 1px solid #CCC;
}

 .wc-day-column {
	width: 13.5%;
	overflow: visible;
	vertical-align: top;
}
 .wc-day-column-header{
	border-width: 0 0 0 1px; 
	border-style: solid;
	border-color:transparent;
}
 .wc-scrollable-grid .wc-day-column-last,
 .wc-scrollable-grid .wc-day-column-middle{
	border-width: 0 0 0 1px; border-style: dashed;
	}
 .wc-scrollable-grid .wc-day-column-first{
	border-left: 1px solid #CCC;
}

 .wc-day-column-inner {
	width: 100%;
	position:relative;	
}

 .wc-no-height-wrapper{
	position:relative;	
  overflow: visible;
	height: 0px;
}

 .wc-time-slot-wrapper {
/*	top: 3px;*/
}
 .wc-oddeven-wrapper .wc-full-height-column{
/*	top: 2px; */
  /* Modern Browsers */ opacity: 0.4;
  /* IE 8 */ -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=40)";
  /* IE 5-7 */ filter: alpha(opacity=40);
  /* Netscape */ -moz-opacity: 0.4;
  /* Safari 1 */ -khtml-opacity: 0.4;
}
 .wc-freebusy-wrapper .wc-freebusy{
/*	top: 1px;*/
  /* Modern Browsers */ opacity: 0.4;
  /* IE 8 */ -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=40)";
  /* IE 5-7 */ filter: alpha(opacity=40);
  /* Netscape */ -moz-opacity: 0.4;
  /* Safari 1 */ -khtml-opacity: 0.4;
}

 .wc-time-slots {
	position: absolute;	
	width: 100%;
}

 .wc-column-odd,  .wc-column-even.ui-state-hover{background-image:none;border:none;}

 .wc-header .wc-today.ui-state-active{background-image:none;}
 .wc-header .wc-today.wc-day-column-header{ 
	border-bottom: 1px solid #68A1E5;
	font-weight: bold;
	color: #0b6bde;
}
 .wc-header .wc-user-header{border-width:0;}

 .wc-time-slots .wc-day-column.ui-state-default{
	background:#fafafa;
}
 .wc-time-slots .wc-today.ui-state-active{
	background:#FFF;
}
 .wc-header .wc-today.ui-state-active.wc-day-column-middle{border-width:0;}
 .wc-header .wc-today.ui-state-active.wc-day-column-first{border-left-width:3px;}
 .wc-header .wc-today.ui-state-active.wc-day-column-last{border-right-width:3px;}

 .wc-full-height-column{
	display:block;
/*  width:100%;*/
}


 .wc-time-header-cell {
	position: relative;	
	height: 80px; /* reference height */
}
 .wc-time-header-cell .hour {
	position:absolute;
	width: 100%;
	top: -7px;
	right: 5px;
}
 .first_hour .wc-time-header-cell .hour {
	top: -1px;
}

 .wc-time-slot {
	border-bottom: 1px dotted #ddd;	
}
 .wc-time-slot:nth-child(even) {
	border-bottom: 1px dotted #ddd;	
}
 .wc-time-slot:nth-child(odd) {
	border-bottom: 1px dotted transparent;	
}

 .wc-hour-header {
	text-align: right;
	font-size: 11px;
	line-height: 11px;
}
 .wc-hour-header .wc-am-pm {
}
 .wc-hour-header.ui-state-active,
 .wc-hour-header.ui-state-default{
  border-width:0 0 1px 0;
}
 .wc-hour-header {
	border-bottom: 1px solid transparent !important;	
}
 .wc-hour-end  {
	border-bottom: 1px solid #ccc !important;	
}

 .wc-business-hours {

}

 .wc-business-hours .wc-am-pm {	
}

 .wc-day-header-cell {
	text-align: center;
	vertical-align: middle;
	padding: 5px;
}



 .wc-time-slot-header .wc-header-cell {
	text-align: right;
	padding-right: 10px;	
}

.wc-cal-event {
	background-color: #086eda;	
  /* Modern Browsers */ opacity: 0.9;
  /* IE 8 */ -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=80)";
  /* IE 5-7 */ filter: alpha(opacity=90);
  /* Netscape */ -moz-opacity: 0.9;
  /* Safari 1 */ -khtml-opacity: 0.9;
	position: absolute;	
	overflow: hidden;
	color: #FFF;
	width: 100%;
	display: none;
	border-radius: 3px;
	text-shadow: 0 -1px 1px #074383;
}
.wc-cal-event.outcome {
	background: #3dab1e;
	text-shadow: 0 -1px 1px #246711;
}
.wc-cal-event.readonly {
	background: #c42438;
	text-shadow: 0 -1px 1px #74131f;
}
.wc-new-cal-event-creating {
	background: #c42438;
}

 .wc-cal-event .wc-time {
	font-weight: bold;
	font-size: 11px;
	padding: 1px 3px 2px;
}

 .wc-container .ui-draggable .wc-time {
}

 .wc-cal-event .wc-title {
	position: relative;	
	font-size: 12px;
	line-height: 1.2;
	padding: 0 3px;
}


 .ui-resizable-s {
	position: absolute;
	height: 7px;
	line-height: 7px;
	width: 100%;
	bottom: 0;
	cursor: ns-resize;
	text-align: center;
	display: none !important;
}
 .wc-cal-event:hover .ui-resizable-s{
	display: block !important;
}
 .ui-resizable-resizing {
	z-index: 9999;
}


 .wc-container .ui-draggable-dragging {
   z-index: 1000;
}

 .free-busy-free{}
 .free-busy-busy{
  background: #666666;
}


 .wc-cal-event.past_event {
	opacity: 0.5;
}


/* IE6 hacks */
* html  .wc-no-height-wrapper{position:absolute;}
* html  .wc-time-slot-wrapper{top:3px;}
* html  .wc-grid-row-oddeven{top:2px;}
* html  .wc-grid-row-freebusy{top:1px;}

/* IE7 hacks */
*:first-child+html  .wc-no-height-wrapper{position:relative;}
*:first-child+html  .wc-time-slot-wrapper{top:3px;}
*:first-child+html  .wc-grid-row-oddeven{top:2px;}
*:first-child+html  .wc-grid-row-freebusy{top:1px;}
*:first-child+html  .wc-time-slots .wc-today{/* due to rendering issues, no background */background:none;}
