#plan_empty {
	width: 248px;
	height: 73px;
	background: url(<%= asset_path 'plan_empty.png'  %>) no-repeat;
	margin: 5px 100px 0 0;
	float: right;
}

#day_plan_wrap {
	margin: 5px 100 0 0;
}
#day_plan_wrap li.outcome {
	margin: 0 0 15px 0;
}



#plan_side #date_section {
	margin: 0 10px;
	padding: 20px 0 15px;
	border-bottom: 1px solid #1F1D1C;
}
#plan_side #date_section .big_date {
	display: inline-block;
	font-size: 60px;
	color: #FFF;
	height: 55px;
	margin: 0 7px 0 0;
}
#plan_side #date_section .small_date_wrap {
	height: 100%;
	position: relative;
	display: inline-block;
	font-size: 14px;
	color: #FFF;
	vertical-align: bottom;
	white-space: nowrap;
}
#plan_side #date_section .small_date_day {
	position: absolute;
	left:0;
	bottom: 20px;
	font-size: 14px;
}
#plan_side #date_section .times_wrap {
	position: absolute;
	left:0;
	bottom: 5px;
	overflow: hidden;
	font-size: 11px;
}
#plan_side #date_section .times_wrap .title {
	font-weight: bold;
	margin: 0 7px 0 0;
	display: inline-block;
	color: #afadac;
}
#plan_side #date_section .times_wrap .time {
	display: inline-block;
}
#plan_side #date_section .times_wrap .must_wrap {
	float: left;
	margin: 0 12px 0 0;
}
#plan_side #date_section .times_wrap .total_wrap {
	float: left;
}


#side_schedule .wc-header{
	height: 0px;
	margin: 0 10px 0 10px;
	border-top: 1px solid #413d3c !important;
}
#side_schedule .wc-header .wc-day-column-header {
	text-align: center;
	font-size: 12px;
	padding: 30px 0 0 0;
}
#side_schedule .wc-header table{
	width: 100%;
  	table-layout:fixed;
	display: none;
}
#side_schedule .wc-grid-timeslot-header, #side_schedule .wc-header .wc-time-column-header {
	width: 20px;	
}


#side_schedule .wc-scrollable-grid {
	overflow: auto;
	overflow-x: hidden !important;
	overflow-y: auto !important; 
	position: absolute;
	top: 90px;
	left: 5px;
	right: 10px;
	bottom: 0;
}

#side_schedule table.wc-time-slots {
	width: 100%;
	table-layout: fixed;
	cursor: default;
	overflow:hidden;
	border-top: 1px solid transparent;
}

#side_schedule .wc-day-column-header{
	border-width: 0 0 0 1px; 
	border-style: solid;
	border-color:transparent;
}
#side_schedule .wc-scrollable-grid .wc-day-column-last,
#side_schedule .wc-scrollable-grid .wc-day-column-middle{
	border:none;
}


#side_schedule .wc-column-odd, #side_schedule .wc-column-even.ui-state-hover{background-image:none;border:none;}

#side_schedule .wc-header .wc-today.ui-state-active{background-image:none;}
#side_schedule .wc-header .wc-today.wc-day-column-header{border-width:0 3px; border-style: solid;}
#side_schedule .wc-header .wc-user-header{border-width:0;}

#side_schedule .wc-time-slots .wc-day-column.ui-state-default{
	background:transparent;
}
#side_schedule .wc-time-slots .wc-today.ui-state-active{
	background: transparent;
}
#side_schedule .wc-header .wc-today.ui-state-active.wc-day-column-middle{border-width:0;}
#side_schedule .wc-header .wc-today.ui-state-active.wc-day-column-first{border-left-width:3px;}
#side_schedule .wc-header .wc-today.ui-state-active.wc-day-column-last{border-right-width:3px;}

#side_schedule .wc-full-height-column{
	display:block;
/*  width:100%;*/
}

#side_schedule .wc-time-header-cell .hour {
	position:absolute;
	width: 100%;
	top: -7px;
	right: 3px;
}
#side_schedule .first_hour .wc-time-header-cell .hour {
	top: -1px;
}

#side_schedule .wc-hour-header {
	border-bottom: 1px solid transparent !important;	
}
#side_schedule .wc-hour-end {
	border-bottom: 1px solid #171615 !important;	
	border-top: 0 !important;
}
#side_schedule .wc-hour-start {
	border-top: 1px solid #413d3c !important;	
	border-bottom: 0 !important;
}
#side_schedule .wc-hour-second {
	border-bottom: 1px dotted #171615 !important;	
	border-top: 0 !important;
}
#side_schedule .wc-hour-third {
	border-top: 1px dotted #413d3c !important;	
	border-bottom: 0 !important;
}


#side_schedule .wc-hour-header {
	text-align: right;
	font-size: 10px;
	color: #afadac;
	font-weight: bold;
}
#side_schedule .wc-hour-header .wc-am-pm {
}
#side_schedule .wc-hour-header.ui-state-active,
#side_schedule .wc-hour-header.ui-state-default{
  border-width:0 0 1px 0;
}

#side_schedule .wc-cal-event {
  /* Modern Browsers */ opacity: 1.0;
  /* IE 8 */ -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=100)";
  /* IE 5-7 */ filter: alpha(opacity=100);
  /* Netscape */ -moz-opacity: 1.0;
  /* Safari 1 */ -khtml-opacity: 1.0;
}
